# Sniperoo Config
SNIPEROO_API_KEY="YOUR_SNIPEROO_API_KEY_HERE"
SNIPEROO_PUBKEY="YOUR_SNIPEROO_PUBKEY_HERE"

# Primary RPC (Helius)
HELIUS_HTTPS_URI="https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_API_KEY_HERE"
HELIUS_WSS_URI="wss://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_API_KEY_HERE"

# Backup RPC endpoints (Shyft)
SHYFT_HTTPS_URI="https://rpc.shyft.to?api_key=YOUR_SHYFT_API_KEY_HERE"
SHYFT_WSS_URI="wss://rpc.shyft.to?api_key=YOUR_SHYFT_API_KEY_HERE"

# Additional Helius backup
HELIUS_BACKUP_HTTPS_URI="https://mainnet.helius-rpc.com/?api-key=YOUR_BACKUP_HELIUS_API_KEY_HERE"
HELIUS_BACKUP_WSS_URI="wss://mainnet.helius-rpc.com/?api-key=YOUR_BACKUP_HELIUS_API_KEY_HERE"

# Public RPC fallback (use only as last resort)
PUBLIC_HTTPS_URI="https://api.mainnet-beta.solana.com"
PUBLIC_WSS_URI="wss://api.mainnet-beta.solana.com"

# Optional: Set provider priority (HELIUS or SHYFT)
PROVIDER="SHYFT" 